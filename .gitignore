*.log
*.sqlite3
*.db
*.bak
*.tmp
.cache/
.pytest_cache/

# Build / packaging
build/
dist/
*.egg-info/
.eggs/

# Testing
coverage.xml
.coverage
htmlcov/

# Crash reports
*.stackdump

# Secrets and config
.env
secrets.yml
credentials.json

